
public class TestProject {
	
	private String name;
	private int age;
	private String department;
	
	public TestProject(String name, int age, String department) {
		
		this.name = name;
		this.age = age;
		this.department = department;
	}
	
	
	public String toString() {
		return name + " " + age + department;
	}
	

}
